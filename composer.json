{
    "name": "app-insights-php/client",
    "description": "Microsoft App Insights telemetry client php wrapper",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Francisco Abayon",
            "email": "franz.noyaba@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests"
        }
    },
    "repositories":
    [
            {
                "type": "vcs",
                "url": "https://github.com/curlybytes/ApplicationInsights-PHP"
            }
    ],
    "require": {
        "php": "^7.1",
        "microsoft/application-insights": "dev-master as 0.4.5",
        "guzzlehttp/guzzle": "^6.0",
        "psr/simple-cache": "^1.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 || ^8.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
